<properties>
<property id="2" name="CommandTimeout" dataType="System.Int32" state="default" isArray="false" description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out." typeConverter="" UITypeEditor="" containsID="false" expressionType="None">0</property>
<property id="3" name="OpenRowset" dataType="System.String" state="default" isArray="false" description="Specifies the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="4" name="OpenRowsetVariable" dataType="System.String" state="default" isArray="false" description="Specifies the variable that contains the name of the database object used to open a rowset." typeConverter="" UITypeEditor="" containsID="false" expressionType="None"></property>
<property id="5" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">select case when numerocuenta in ('121001', '1212001') then '1212001' else '1212003' end as numerocuenta, fechaemision as fechadocumento, 
DP.seriefactura as serie, to_char(DP.numerofactura) as documento, 
to_char(nvl(periodocontrato, '')) || '-' || to_char(nvl(numerocontrato, '')) || '-' || to_char(nvl(numeroampliacion, '')) as contrato, 
DD.codigopersona, nombrecompleto,
sum(nvl(debesoles, 0) - nvl(habersoles, 0)) as saldosoles, sum(nvl(debedolares, 0) - nvl(haberdolares, 0)) as saldodolares
from siscomdba.xlibrodiario D, siscomdba.xlibrodiariodetalle DD, siscomdba.persona P, siscomdba.documentopendiente DP 
where D.periodolibro = DD.periodolibro and D.codigolibro = DD.codigolibro and DD.codigopersona = P.codigopersona
and ltrim(DD.numerodocumento, '0') = to_char(DP.numerofactura) and DP.estado = 1 and tipoasiento not in (51)
and D.codigolibro &gt; 1 and numerocuenta in ('121001', '1212001', '121003', '1212003')
and fechaemision between to_date('01/01/2008','dd/mm/yyyy') and to_date('31/12/2011','dd/mm/yyyy')
and fechaasiento &lt;= to_date('31/12/2011','dd/mm/yyyy') and observaciones1 is null
group by case when numerocuenta in ('121001', '1212001') then '1212001' else '1212003' end,
fechaemision, DP.seriefactura, DP.numerofactura, 
to_char(nvl(periodocontrato, '')) || '-' || to_char(nvl(numerocontrato, '')) || '-' || to_char(nvl(numeroampliacion, '')),
DD.codigopersona, nombrecompleto
having sum(nvl(debedolares, 0) - nvl(haberdolares, 0)) &gt; 0
union
select '1231001' as numerocuenta, fechavencimiento as fechadocumento, 
DP.seriefactura as serie, DP.documentoreferencia as documento, 
to_char(nvl(periodocontrato, '')) || '-' || to_char(nvl(numerocontrato, '')) || '-' || to_char(nvl(numeroampliacion, '')) as contrato, 
DD.codigopersona, nombrecompleto,
sum(nvl(debesoles, 0) - nvl(habersoles, 0)) as saldosoles, sum(nvl(debedolares, 0) - nvl(haberdolares, 0)) as saldodolares
from siscomdba.xlibrodiario D, siscomdba.xlibrodiariodetalle DD, siscomdba.persona P, siscomdba.documentopendiente DP 
where D.periodolibro = DD.periodolibro and D.codigolibro = DD.codigolibro and DD.codigopersona = P.codigopersona
and DD.numerodocumento = DP.documentoreferencia and DP.estado = 1 and tipoasiento not in (51) 
and D.codigolibro &gt; 1 and numerocuenta in ('123001', '1231001') 
and (fechavencimiento &gt;= to_date('01/01/2011','dd/mm/yyyy') or documentoreferencia = '20071263')
and fechaasiento &lt;= to_date('31/12/2011','dd/mm/yyyy')
group by fechavencimiento, DP.seriefactura, DP.documentoreferencia, 
to_char(nvl(periodocontrato, '')) || '-' || to_char(nvl(numerocontrato, '')) || '-' || to_char(nvl(numeroampliacion, '')),
DD.codigopersona, nombrecompleto
having sum(nvl(debedolares, 0) - nvl(haberdolares, 0)) &lt;&gt; 0</property>
